{
    "version": 2,
    "builds": [
      {
        "src": "src/contact.js",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["src/**"] 
        }
      }
    ],
    "routes": [
      {
        "src": "/api/contact",
        "dest": "src/contact.js",
        "methods": ["POST"]
      },
      {
        "src": "/api/health",
        "dest": "src/contact.js",
        "methods": ["GET"]
      },
      {
        "src": "/(.*)",
        "status": 404,
        "dest": "src/contact.js"
      }
    ]
  }